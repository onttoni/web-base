{ "name" : "web-base",
  "version" : "0.0.0",
  "description" : "Common Node.js modules for web-base.",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery: jQuery"
      ]
    }
  },
  "dependencies" : {
    "angular": "latest",
    "angular-modal-service": "latest",
    "angular-resource": "latest",
    "angular-route": "latest",
    "angular-sanitize": "latest",
    "bcrypt-nodejs": "latest",
    "body-parser": "latest",
    "bootstrap": "latest",
    "bunyan": "latest",
    "connect-mongo": "latest",
    "express": "latest",
    "express-bunyan-logger": "latest",
    "express-session": "latest",
    "font-awesome": "latest",
    "jquery": "latest",
    "lodash": "latest",
    "method-override": "latest",
    "mongoose": "latest",
    "passport": "latest",
    "passport-local": "latest"
  },
  "devDependencies": {
    "browserify": "latest",
    "browserify-shim": "latest",
    "grunt": "latest",
    "grunt-browserify": "latest",
    "grunt-cli": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-cssmin": "latest",
    "grunt-contrib-htmlmin": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-html-angular-validate": "latest",
    "grunt-mkdir": "latest",
    "grunt-ng-annotate": "latest",
    "nodemon": "latest"
  },
  "scripts": {
    "start": "./nodemon -w src/server/ src/server/server.js |./bunyan -o short -L"
  }
}
