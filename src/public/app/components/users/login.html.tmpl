<h2>Login</h2>
<a ng-href="#/">Show Home</a>

<div>
  <form novalidate>
    <div class="form-group">
      <label class="">Email</label>
      <input type="text" class="form-control" ng-model="users.loggedIn.email">
    </div>
    <div class="form-group">
      <label class="">Password</label>
      <input type="text" class="form-control" ng-model="users.loggedIn.password">
    </div>
    <div>
      <a ng-href="#/" class="btn btn-default" ng-click="users.login()"><i class="fa fa-floppy-o"></i>Save</a>
      <a ng-href="#/" class="btn btn-default"><i class="fa fa-times"></i>Cancel</a>
    </div>
  </form>
</div>
